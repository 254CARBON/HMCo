[run]
source = services
omit = 
    */tests/*
    */venv/*
    */virtualenv/*
    */.venv/*
    setup.py
    */site-packages/*

[report]
precision = 2
show_missing = True
skip_covered = False

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract

[html]
directory = test-reports/coverage-html

[xml]
output = test-reports/coverage.xml


